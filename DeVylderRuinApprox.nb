Cell[BoxData[
 RowBox[{
  RowBox[{"ruinDirectILT", "[", 
   RowBox[{"Fx_", ",", "rho_", ",", "x_", ",", "u_"}], "]"}], ":=", " ", 
  RowBox[{"Module", "[", 
   RowBox[{
    RowBox[{"{", 
     RowBox[{
     "s", ",", "L1", ",", "f", ",", "m1", ",", " ", "Lf", ",", " ", "fe", ",",
       "Fe", ",", "Lrui"}], "}"}], ",", "\[IndentingNewLine]", 
    RowBox[{
     RowBox[{
      RowBox[{"f", "[", "x", "]"}], " ", "=", " ", 
      RowBox[{"-", 
       RowBox[{"D", "[", 
        RowBox[{
         RowBox[{"Fx", "[", "x", "]"}], ",", "x"}], "]"}]}]}], ";", 
     "\[IndentingNewLine]", 
     RowBox[{"m1", " ", "=", " ", 
      RowBox[{"Integrate", "[", 
       RowBox[{
        RowBox[{"Fx", "[", "x", "]"}], ",", 
        RowBox[{"{", 
         RowBox[{"x", ",", "0", ",", "\[Infinity]"}], "}"}]}], "]"}]}], ";", 
     "\[IndentingNewLine]", 
     RowBox[{"Lf", " ", "=", " ", 
      RowBox[{"LaplaceTransform", "[", 
       RowBox[{
        RowBox[{"Fx", "[", "x", "]"}], ",", "x", ",", "s"}], "]"}]}], ";", 
     "\[IndentingNewLine]", 
     RowBox[{"fe", " ", "=", " ", 
      RowBox[{"Lf", "/", "m1"}]}], ";", "\[IndentingNewLine]", 
     RowBox[{"Fe", "=", 
      RowBox[{"Factor", "[", 
       RowBox[{"(", 
        RowBox[{
         RowBox[{"(", 
          RowBox[{"1", "-", "fe"}], ")"}], "/", "s"}], ")"}], "]"}]}], ";", 
     "\[IndentingNewLine]", 
     RowBox[{"Lrui", "=", 
      RowBox[{"rho", "*", 
       RowBox[{"Fe", "/", 
        RowBox[{"(", 
         RowBox[{"1", "-", 
          RowBox[{"rho", "*", "fe"}]}], ")"}]}]}]}], ";", 
     "\[IndentingNewLine]", 
     RowBox[{"L1", " ", "=", 
      RowBox[{"InverseLaplaceTransform", "[", 
       RowBox[{"Lrui", ",", "s", ",", "u"}], "]"}]}]}]}], 
   "\[IndentingNewLine]", "]"}]}]], "Input",
