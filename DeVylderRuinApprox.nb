Cell["Another hyperexponential claim, sum of 5 exponentials.", "Text",
 CellChangeTimes->{{3.7969573308719983`*^9, 
  3.7969573637830653`*^9}},ExpressionUUID->"3613f48b-df65-4ac4-92e7-\
092c470db7ce"],

Cell[CellGroupData[{

Cell[BoxData[{
 RowBox[{
  RowBox[{"r", "=", 
   RowBox[{"{", 
    RowBox[{"1", ",", "3", ",", "5", ",", "7", ",", "9"}], "}"}]}], ";", 
  RowBox[{"\[Beta]", "=", 
   RowBox[{"{", 
    RowBox[{"2", ",", "4", ",", "6", ",", "8", ",", "10"}], "}"}]}], ";", " ", 
  RowBox[{"n", " ", "=", " ", 
   RowBox[{"Length", "[", "r", "]"}]}], ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"varC", "=", 
   RowBox[{"Table", "[", 
    RowBox[{
     RowBox[{"Symbol", "[", 
      RowBox[{"\"\<varC\>\"", "<>", 
       RowBox[{"ToString", "[", "i", "]"}]}], "]"}], ",", 
     RowBox[{"{", 
      RowBox[{"i", ",", "1", ",", "n"}], "}"}]}], "]"}]}], 
  ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"condC", " ", "=", " ", 
   RowBox[{"Table", "[", 
    RowBox[{
     RowBox[{
      RowBox[{
       SubsuperscriptBox["\[Sum]", 
        RowBox[{"k", "=", "1"}], 
        RowBox[{"k", "=", "n"}]], 
       FractionBox[
        RowBox[{"varC", "[", 
         RowBox[{"[", "k", "]"}], "]"}], 
        RowBox[{
         RowBox[{"\[Beta]", "[", 
          RowBox[{"[", "i", "]"}], "]"}], "-", " ", 
         RowBox[{"r", "[", 
          RowBox[{"[", "k", "]"}], "]"}]}]]}], " ", "\[Equal]", 
      FractionBox["1", 
       RowBox[{"\[Beta]", "[", 
        RowBox[{"[", "i", "]"}], "]"}]]}], ",", 
     RowBox[{"{", 
      RowBox[{"i", ",", "n"}], "}"}]}], "]"}]}], ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"s", " ", "=", " ", 
   RowBox[{"Solve", "[", 
    RowBox[{"condC", ",", " ", "varC"}], "]"}]}], 
  ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"c", "=", 
   RowBox[{"Flatten", "[", 
    RowBox[{"varC", "/.", 
     RowBox[{"{", "s", "}"}]}], "]"}]}], ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"\[Rho]", " ", "=", " ", 
   RowBox[{"Total", "[", "c", "]"}]}], ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"\[Theta]", " ", "=", " ", 
   RowBox[{
    RowBox[{"\[Rho]", "^", 
     RowBox[{"(", 
      RowBox[{"-", "1"}], ")"}]}], " ", "-", "1"}]}], 
  ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"varA", "=", 
   RowBox[{"Table", "[", 
    RowBox[{
     RowBox[{"Symbol", "[", 
